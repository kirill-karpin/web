{
  "compilerOptions": {
    "pretty": true,
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "lib": ["es6", "dom"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "jsx": "react",
    "types": []
  },
  "exclude": [
    "/node_modules/"
  ]
}
